{
    "workbench.colorTheme": "Visual Studio Dark",
    "cSpell.words": [
        "Ajmal",
        "camelcase",
        "Kenx",
        "Nasumdeen"
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        {
            "language": "typescript",
            "autoFix": true
        },
        {
            "language": "typescriptreact",
            "autoFix": true
        }
    ]
}